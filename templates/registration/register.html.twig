
{% block title %}Register{% endblock %}

{% block body %}


{% block css %}


    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="{{asset('formAdmin/images/icons/favicon.ico')}}"/>
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="{{asset('formAdmin/vendor/bootstrap/css/bootstrap.min.css')}}">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="{{asset('formAdmin/fonts/font-awesome-4.7.0/css/font-awesome.min.css')}}">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="{{asset('formAdmin/vendor/animate/animate.css')}}">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="{{asset('formAdmin/vendor/css-hamburgers/hamburgers.min.css')}}">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="{{asset('formAdmin/vendor/select2/select2.min.css')}}">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="{{asset('formAdmin/css/util.css')}}">
    <link rel="stylesheet" type="text/css" href="{{asset('formAdmin/css/main.css')}}">
    <!--===============================================================================================-->

{% endblock %}
<body>

<div class="limiter">
    <div class="container-login100">
        <div class="wrap-login100">
            <div class="login100-pic js-tilt" data-tilt>
                <img src="{{asset('formAdmin/images/img-01.png')}}" alt="IMG">
            </div>
            {% for flashError in app.flashes('verify_email_error') %}
                <div class="alert alert-danger" role="alert">{{ flashError }}</div>
            {% endfor %}




                {{ form_start(registrationForm, {'attr': {'novalidate': 'novalidate','class':'login100-form validate-form'}}) }}
            <span class="login100-form-title">
						Member Login
					</span>
                {#  {{ form_row(registrationForm.email) }}#}
                {# {{ form_row(registrationForm.plainPassword, {  label: 'Password'       }) }} #}
                {{ form_row(registrationForm.agreeTerms) }}

                <div class="wrap-input100 validate-input" data-validate = "Valid email is required: ex@abc.xyz">
                   {#  <input class="input100" type="text" name="email" placeholder="Email">#}
                    {{ form_widget(registrationForm.email, {'attr': {'class': 'input100'}}) }}
                    <span class="focus-input100"></span>
                    <span class="symbol-input100">
							<i class="fa fa-envelope" aria-hidden="true"></i>
						</span>
                </div>

                <div class="wrap-input100 validate-input" data-validate = "Password is required">
                    {# <input class="input100" type="password" name="pass" placeholder="Password"> #}
                    {{form_widget(registrationForm.plainPassword, {'attr': {'class': 'input100'}}) }}
                    <span class="focus-input100"></span>
                    <span class="symbol-input100">
							<i class="fa fa-lock" aria-hidden="true"></i>
						</span>
                </div>

                <div class="container-login100-form-btn">
                   {#  <button class="login100-form-btn" type="submit"> Login     </button>#}
                    {{form_widget(registrationForm.valider, {'attr': {'class': 'login100-form-btn'}}) }}
                </div>
            {{ form_end(registrationForm) }}
                <div class="text-center p-t-12">
						<span class="txt1">
							Forgot
						</span>
                    <a class="txt2" href="#">
                        Username / Password?
                    </a>
                </div>

                <div class="text-center p-t-136">
                    <a class="txt2" href="#">
                        Create your Account
                        <i class="fa fa-long-arrow-right m-l-5" aria-hidden="true"></i>
                    </a>
                </div>

        </div>
    </div>
</div>




<!--===============================================================================================-->
<script src="{{asset('formAdmin/vendor/jquery/jquery-3.2.1.min.js')}}"></script>
<!--===============================================================================================-->
<script src="{{asset('formAdmin/vendor/bootstrap/js/popper.js')}}"></script>
<script src="{{asset('formAdmin/vendor/bootstrap/js/bootstrap.min.js')}}"></script>
<!--===============================================================================================-->
<script src="{{asset('formAdmin/vendor/select2/select2.min.js')}}"></script>
<!--===============================================================================================-->
<script src="{{asset('formAdmin/vendor/tilt/tilt.jquery.min.js')}}"></script>
<script >
    $('.js-tilt').tilt({
        scale: 1.1
    })
</script>
<!--===============================================================================================-->
<script src="{{asset('formAdmin/js/main.js')}}"></script>


{% endblock %}






